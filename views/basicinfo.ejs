        onClose: function (obj) {
            $("#hometown-city").select('update', {input: '请选择'});
            //更新city select items
            (function (pid, cb) {
                var currentCitys = [];
                citys.forEach(function (city) {
                    if (city.pid == pid) {
                        currentCitys.push(city);
                    }
                })
                cb(currentCitys);
            })(obj.data.values, function (currentCitys) {
                $("#hometown-city").select('update', {items: currentCitys});
            });

        }
    });


