                <!--</div>-->
                <div class="weui_cell">
                    <div class="weui_cell_hd"><label for="name" class="weui_label">家乡省份</label></div>
                    <div class="weui_cell_bd weui_cell_primary">
                        <input class="weui_input" id="hometown-province" type="text" value="" readonly="">
                    </div>
                </div>

                <div class="weui_cell">
                    <div class="weui_cell_hd"><label for="name" class="weui_label">家乡城市</label></div>
                    <div class="weui_cell_bd weui_cell_primary">
                        <input class="weui_input" id="hometown-city" type="text" value="" readonly="">
                    </div>
                </div>

            </div>
        </div>

        <div class="weui_cells">

            <div class="weui_cell">

                <div class="weui_cell_bd weui_cell_primary">
